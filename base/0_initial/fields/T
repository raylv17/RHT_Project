dimensions      [0 0 0 1 0 0 0];

internalField   uniform 1000.0;

boundaryField
{
  ".*WW.*"
    {
      type            fixedValue;
        value           uniform 1000.0;
    }
".*SYM.*"
  {
    type            symmetryPlane;
  }
}


