dimensions      [0 0 0 0 0 0 0];

// internalField   ${:internalField.nonuniform.CO};
internalField   uniform 0.0;
boundaryField
{
  ".*WW.*"
    {
        type            zeroGradient;
    }
".*SYM.*"
  {
    type            symmetryPlane;
  }

}


